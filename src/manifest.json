{
    "manifest_version": 3,
    "name": "AI Detector Pro",
    "version": "1.0.0",
    "description": "Real-time AI content detection using serverless inference.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }